{
  "name": "Bookmark Youtube Playlists",
  "version": "1.0",
  "description": "This extension will help you to create youtube playlists from bookmark folders.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    }
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "permissions": ["bookmarks"]
}
